<div ng-controller="indexCtrl">
  <h1>Home#index</h1>
  <p>Find me in app/views/home/index.html.erb</p>

  <ng-tag-cloud cloud-width="250" cloud-height="250" cloud-data="data"></ng-tag-cloud>

  <div ng-repeat="photo in photos">
    <div class="top-photo">
      <a ng-href="/users/{{photo.user_id}}"><img ng-src="{{photo.url}}"/></a>

    </div>
  </div>
</div>
